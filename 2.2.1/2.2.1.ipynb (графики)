    "sum_x2 = np.sum(x1**2)\n",
    "sum_x = np.sum(x1)\n",
    "Delta = N * sum_x2 - sum_x**2\n",
    "sigma_k = np.sqrt(np.sum(t**2) / Delta)  # Теперь учитываются реальные погрешности\n",
    "\n",
    "# Линия аппроксимации\n",
    "y_line = a2 * x_line + b\n",
    "plt.plot(x_line, y_line, label=f\"y = ({a2:.4f} ± {sigma_k:.4f})x + {b:.2f}\", color='blue')\n",
    "\n",
    "# Добавление погрешностей к точкам без обводки\n",
    "#plt.errorbar(x1, y, xerr = 0.001,yerr=t, fmt='', color='black', capsize=2.5)\n",
    "\n",
    "# Включение сетки\n",
    "plt.grid()\n",
    "\n",
    "# Подписи осей и заголовок\n",
    "plt.xlabel(\"t, сек\")\n",
    "plt.ylabel(\"ln(U), мВ\")\n",
    "plt.title(\"Зависимость натурального логарифма напряжения от t, P = 58.8 торр\")\n",
    "\n",
    "plt.legend()\n",
    "\n",
    "# Корректное сохранение файла\n",
    "plt.savefig(r'C:\\prog\\Zavisimost_U_ot_t_58_8.png')\n",
    "\n",
    "plt.show()\n"
   ]
