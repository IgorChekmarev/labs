    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "import math\n",
    "\n",
    "# Данные\n",
    "\n",
    "x1 = np.array([1/40.4, 1/77.2, 1/99.2, 1/117.6])\n",
    "y = np.array([9.86, 5.55, 4.31, 4.11])\n",
    "t =  np.array([0.28, 0.16, 0.12, 0.12]) # Погрешность (массив той же длины, что y)\n",
    "\n",
    "\n",
    "# Линия для интерполяции\n",
    "x_line = np.linspace(min(x1), max(x1), 1000)\n",
    "\n",
    "# Интерполяция значений y\n",
    "y_interp = np.interp(x_line, x1, y)\n",
    "\n",
    "# Интерполяция погрешностей\n",
    "t_interp = np.interp(x_line, x1, t)\n",
    "\n",
    "# Закрашивание области погрешностей\n",
    "#plt.fill_between(x_line, y_interp - t_interp, y_interp + t_interp, alpha=0.3, color='green', label='Погрешность')\n",
    "\n",
    "# Построение точек без обводки\n",
    "plt.scatter(x1, y, color='red', s=25, zorder=3)\n",
    "\n",
    "# Линейная аппроксимация\n",
    "a3, b = np.polyfit(x1, y, 1)\n",
    "\n",
    "# Расчёт погрешности углового коэффициента\n",
    "N = len(x1)\n",
    "sum_x2 = np.sum(x1**2)\n",
    "sum_x = np.sum(x1)\n",
    "Delta = N * sum_x2 - sum_x**2\n",
    "sigma_k = np.sqrt(np.sum(t**2) / Delta)  # Теперь учитываются реальные погрешности\n",
    "\n",
    "# Линия аппроксимации\n",
    "y_line = a3 * x_line + b\n",
    "plt.plot(x_line, y_line, label=f\"y = ({a3:.4f} ± {sigma_k:.4f})x + {b:.2f}\", color='blue')\n",
    "\n",
    "# Добавление погрешностей к точкам без обводки\n",
    "#plt.errorbar(x1, y, xerr = 0.001,yerr=t, fmt='', color='black', capsize=2.5)\n",
    "\n",
    "# Включение сетки\n",
    "plt.grid()\n",
    "\n",
    "# Подписи осей и заголовок\n",
    "plt.xlabel(\"P, торр^-1\")\n",
    "plt.ylabel(\"D, см^2/c\")\n",
    "plt.title(\"Зависимость коэффициента диффузии D от P^-1\")\n",
    "\n",
    "plt.legend()\n",
    "\n",
    "# Корректное сохранение файла\n",
    "plt.savefig(r'C:\\prog\\Zavisimost_D_P_.png')\n",
    "\n",
    "plt.show()\n"
